{
    "name": "devboard/github-doctrine-types",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "comment": "Adding my own multiline fixers",
            "url": "https://github.com/msvrtan/PHP-CS-Fixer.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "DevboardLib\\GitHubDoctrineType\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "devboard/lib-github": "^0.2.0",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "ciaranmcnulty/phpspec-typehintedmethods": "^3.0",
        "friendsofphp/php-cs-fixer": "dev-long-line-fixers",
        "infection/infection": "^0.8.1",
        "leanphp/phpspec-code-coverage": "^4.0",
        "mockery/mockery": "^1.0",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "phpspec/phpspec": "^4.2",
        "phpstan/phpstan": "^0.9.0",
        "phpstan/phpstan-phpunit": "^0.9.0",
        "phpunit/phpunit": "^6.4",
        "slevomat/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.0"
        }
    },
    "extra": {
    }
}
